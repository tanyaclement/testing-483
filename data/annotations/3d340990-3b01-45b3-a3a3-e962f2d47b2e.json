      "uuid": "a9c23153-8ef2-4ab9-9b30-01b4e4b11543"
    },
    {
      "start_time": 45,
      "end_time": 46,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "With your bitter, twisted lies,"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Tempo",
          "category": "Performance"
        },
        {
          "tag": "Transcription",
          "category": "Background"
        }
