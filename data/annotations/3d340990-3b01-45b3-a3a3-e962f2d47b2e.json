      "uuid": "a7bcbf51-c434-4281-bc01-23221e474eb1"
    },
    {
      "start_time": 9,
      "end_time": 11,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "And yet each of us has awakened or risen"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Speech",
          "category": "Extra-Poetic Sounds"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
        },
