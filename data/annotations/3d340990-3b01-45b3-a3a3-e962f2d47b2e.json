      "end_time": 58,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Why are you beset with gloom?"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Tempo",
          "category": "Performance"
        },
        {
          "category": "_uncategorized_",
