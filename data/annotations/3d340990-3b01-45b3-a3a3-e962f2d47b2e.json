      "end_time": 72,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Just like hopes springing high,"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Tempo",
          "category": "Performance"
        },
        {
          "tag": "Transcription",
